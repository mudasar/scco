              Margin="20,100,20,0"
              VerticalAlignment="Top">
            <Rectangle x:Name="Rectangle1"
                       Fill="#7F284B7E"
                       RadiusX="5"
                       RadiusY="5"
                       Stroke="#7F6A94D4" />
