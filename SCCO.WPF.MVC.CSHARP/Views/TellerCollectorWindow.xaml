                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="90" />
                    <ColumnDefinition />
                    <ColumnDefinition Width="90" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Label x:Name="lblCaptionMemberCode"
                       Height="26"
                       Margin="4,2,2,2"
                       VerticalAlignment="Top"
                       Content="Code :"
                       Foreground="#FF6A94D4"
                       Padding="0" />
                <TextBox x:Name="txtMemberCode"
                         Grid.Column="1"
                         Height="26"
                         Margin="2"
                         VerticalAlignment="Top"
                         VerticalContentAlignment="Center"
                         Background="{x:Null}"
                         BorderBrush="#19F5F5F5"
                         FontSize="16"
                         Foreground="WhiteSmoke"
                         Padding="0"
                         SelectionBrush="Yellow"
                         Text="{Binding MemberCode}" />
