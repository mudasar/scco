<Views:BaseWindow x:Class="SCCO.WPF.MVC.CS.Views.TellerCollectorWindow"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:Models="clr-namespace:SCCO.WPF.MVC.CS.Models"
                  xmlns:Views="clr-namespace:SCCO.WPF.MVC.CS.Views"
                  FontFamily="Ubuntu"
                  SizeToContent="WidthAndHeight"
                  Style="{DynamicResource BaseWindow}"
                  WindowStartupLocation="CenterScreen">

    <Views:BaseWindow.DataContext>
        <Models:TellerCollector />
    </Views:BaseWindow.DataContext>

    <Grid x:Name="Grid" Margin="5">
        <Grid x:Name="FormLayout">
            <Rectangle x:Name="Canvass"
                       Fill="#FF1F497D"
                       MouseLeftButtonDown="DragWindow"
                       RadiusX="5"
                       RadiusY="5"
                       Stroke="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}"
                       StrokeLineJoin="Round">
                <Rectangle.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="0" />
                </Rectangle.Effect>
            </Rectangle>
            <Border x:Name="ShineEffect"
                    Height="16"
                    Margin="2,2,2,0"
                    VerticalAlignment="Top"
                    CornerRadius="4"
                    MouseLeftButtonDown="DragWindow"
                    Style="{DynamicResource ShineEffectStyle}" />
            <Image x:Name="WindowIcon"
                   Width="24"
                   Height="24"
                   Margin="10,10,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   Source="/SCCO.WPF.MVC.CS;component/Images/teller_collector.png"
                   Stretch="Fill" />
            <Label x:Name="FormTitle"
                   Margin="70,35,0,0"
                   Content="Teller / Collector"
                   MouseLeftButtonDown="DragWindow"
                   Style="{StaticResource FormTitle1}" />
            <Button x:Name="CloseButton"
                    Margin="0,8,8,0"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Background="#00E4D2D2"
                    Click="CloseWindow"
                    Content="x"
                    IsCancel="True"
                    Style="{DynamicResource CloseButton}"
                    TabIndex="5" />
        </Grid>
        <Label x:Name="lblCaptionVoucherNumber"
               Height="30"
               Margin="0,50,148,0"
               HorizontalAlignment="Right"
               VerticalAlignment="Top"
               VerticalContentAlignment="Center"
               Content="OR No. :"
               Foreground="#FF6A94D4"
               IsTabStop="True"
               Padding="2"
               Style="{DynamicResource FormHeader1}" />
        <TextBox x:Name="txtVoucherNumber"
                 Width="115"
                 Height="30"
                 Margin="0,50,20,0"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top"
                 VerticalContentAlignment="Center"
                 Background="{x:Null}"
                 BorderBrush="#19F5F5F5"
                 FontSize="21.333"
                 Foreground="WhiteSmoke"
                 LostFocus="txtVoucherNumber_LostFocus"
                 Padding="0"
                 Text="{Binding VoucherNumber}" />
