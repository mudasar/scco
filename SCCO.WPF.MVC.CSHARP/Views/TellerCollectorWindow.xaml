                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnCancelled_Click"
                    Content="Cancelled"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnDelete"
                    Grid.Row="1"
                    Grid.Column="4"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnDelete_Click"
                    Content="Delete"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnClose"
                    Grid.Row="1"
                    Grid.Column="6"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnClose_Click"
                    Content="Close"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnCash"
                    Grid.Row="1"
                    Grid.Column="8"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnCash_Click"
                    Content="Cash"
                    IsEnabled="False"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnCheck"
                    Grid.Row="1"
                    Grid.Column="9"
                    Width="Auto"
                    Margin="1,0"
                    Content="Check"
                    IsEnabled="False"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnDcrDetals"
                    Grid.Row="2"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnDcrDetals_Click"
                    Content="DCR-Details"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnDcrSummary"
                    Grid.Row="2"
                    Grid.Column="2"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnDcrSummary_Click"
                    Content="DCR-Summary"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnPosted"
                    Grid.Row="2"
                    Grid.Column="4"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnPosted_Click"
                    Content="Posted"
                    IsEnabled="False"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnDeposited"
                    Grid.Row="2"
                    Grid.Column="6"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Content="Deposited"
                    IsEnabled="False"
                    Style="{DynamicResource ControlButton}" />
            <Button x:Name="btnTdDetails"
                    Grid.Row="2"
                    Grid.Column="8"
                    Grid.ColumnSpan="2"
                    Width="Auto"
                    Margin="1,0"
                    Click="btnTdDetails_Click"
                    Content="TD Details"
                    Style="{DynamicResource ControlButton}" />
        </Grid>
        <CheckBox x:Name="chkSkipAutomation"
                  Height="25"
                  Margin="0,0,21,11"
                  HorizontalAlignment="Right"
                  VerticalAlignment="Bottom"
                  Content="Skip Automation"
                  Foreground="White"
                  Style="{DynamicResource ControlCheckBox}" />
        <Label x:Name="CancelledLabel"
               Margin="100,253,100,270"
               HorizontalAlignment="Center"
               VerticalAlignment="Center"
               HorizontalContentAlignment="Center"
               VerticalContentAlignment="Center"
               Background="{x:Null}"
               Content="CANCELLED"
               FontSize="64"
               FontWeight="Bold"
               Foreground="#E5FD4141"
               Padding="1"
               RenderTransformOrigin="0.5,0.5"
               Visibility="Hidden">
            <Label.Effect>
                <DropShadowEffect />
            </Label.Effect>
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform Angle="340" />
                    <TranslateTransform />
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Image x:Name="imgLocked"
               Width="48"
               Height="48"
               Margin="20,34,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Source="/SCCO.WPF.MVC.CS;component/Images/lock.png"
               Stretch="Fill" />
        <Image x:Name="imgUnlocked"
               Width="48"
               Height="48"
               Margin="20,34,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               RenderTransformOrigin="0.5,0.5"
               Source="/SCCO.WPF.MVC.CS;component/Images/unlock.png"
               Stretch="Fill"
               Visibility="Hidden" />
    </Grid>
</Views:BaseWindow>