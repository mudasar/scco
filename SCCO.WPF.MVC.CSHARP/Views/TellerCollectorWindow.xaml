                       Grid.Column="3"
                       Margin="2"
                       BorderBrush="#19F5F5F5"
                       BorderThickness="1"
                       Content="{Binding VoucherDate,
                                         StringFormat=\{0:d\}}"
                       FontSize="14.667"
                       Foreground="WhiteSmoke"
                       Padding="3,0,0,3" />
                <Label x:Name="lblCaptionMemberName"
                       Grid.Row="1"
                       Height="26"
                       Margin="4,2,2,2"
                       Content="Name :"
                       Foreground="#FF6A94D4"
                       Padding="0" />
